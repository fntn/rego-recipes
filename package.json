{
  "name": "@fntn/rego-recipes",
  "version": "1.10.1",
  "description": "Cloudsmith EPM Rego samples",
  "scripts": {
    "generate-manifest": "node scripts/generate-manifest.cjs",
    "test": "jest",
    "test:unit": "jest scripts/__tests__/generate-manifest.test.js",
    "test:integration": "jest scripts/__tests__/generate-manifest.integration.test.js",
    "prepare": "husky"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/scripts/__tests__/**/*.test.js"
    ]
  },
  "exports": {
    "./manifest": {
      "types": "./manifest.d.ts",
      "default": "./manifest.json"
    }
  },
  "files": [
    "recipe-*/**/*.rego",
    "huggingface-recipes/**/*",
    "manifest.json",
    "manifest.d.ts",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fntn/rego-recipes.git"
  },
  "keywords": [
    "rego",
    "policy",
    "cloudsmith",
    "epm",
    "open-policy-agent"
  ],
  "author": "Cloudsmith",
  "license": "MIT",
  "devDependencies": {
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "mock-fs": "^5.5.0"
  }
}
